<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" prefHeight="167.0" prefWidth="228.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.IdentificationController">

    <VBox>
        <HBox>
            <Label text="     Name  " />
            <TextField fx:id="textF" />
         <padding>
            <Insets bottom="5.0" />
         </padding>
        </HBox>

        <HBox>
            <Label text="Password " />
            <PasswordField fx:id="passF" />
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
        </HBox>

        <Button fx:id="direc" alignment="CENTER" onAction="#direct" prefHeight="25.0" prefWidth="205.0" text="Directory client">
         <opaqueInsets>
            <Insets bottom="3.0" />
         </opaqueInsets>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin></Button>

        <Button fx:id="btnAuto" onAction="#getIdentity" prefHeight="25.0" prefWidth="205.0" text="Enter the Cloud">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin></Button>

        <Label fx:id="labelStatus" prefHeight="29.0" prefWidth="205.0" />

    </VBox>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>

</GridPane>
